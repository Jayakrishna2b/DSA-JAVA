package com.krishna;

import java.util.Arrays;

public class maxval {
    public static void main(String[] args) {
        int[] arr ={34,25,5,4,424,564,224,64};
        System.out.println(maxval(arr));
    }
    static int maxval(int[] arr){
        int maxval = arr[0];
        for (int i = 1; i <arr.length ; i++) {
            if(arr[i]>maxval){
                maxval = arr[i];
            }
            
        }
        return maxval;
    }
}
